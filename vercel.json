{
  "rewrites": [
    { 
      "source": "/dashboard", 
      "destination": "/index.html" 
    },
    { 
      "source": "/login", 
      "destination": "/biostorelogin.html" 
    },
    { 
      "source": "/images/:path*", 
      "destination": "/images/:path*" 
    },
    {
      "source": "/:slug((?!.*\\.).*)", 
      "destination": "/store.html"
    }
  ]
}
